    "global": {
      "anchors": [
        {
          "anchor": "GitHub Discussions",
          "href": "https://github.com/open-finance-os/discussions",
          "icon": "comments"
        },
        {
          "anchor": "Hackathon",
          "href": "https://preview--uae-open-finance-hackathon.lovable.app/",
          "icon": "trophy"
        },
        {
          "anchor": "API Docs",
          "href": "https://docs.openfinance.ae",
          "icon": "book-open-cover"
        }
      ]
    }
