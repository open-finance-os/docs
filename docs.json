{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "UAE Open Finance Hackathon",
  "colors": {
    "primary": "#DC2626",
    "light": "#EF4444",
    "dark": "#991B1B"
  },
  "favicon": "/adcb-favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Quick Start",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          }
        ]
      },
      {
        "tab": "Starter Templates",
        "groups": [
          {
            "group": "Ready-to-Use Templates",
            "pages": [
              "templates/index",
              "templates/react-nextjs",
              "templates/nodejs-express",
              "templates/python-fastapi",
              "templates/react-native"
            ]
          },
          {
            "group": "Examples & Demos",
            "pages": [
              "examples/index",
              "examples/personal-finance",
              "examples/payment-app",
              "examples/insurance-dashboard",
              "examples/business-banking",
              "examples/mobile-app"
            ]
          }
        ]
      },
      {
        "tab": "Advanced Integration",
        "groups": [
          {
            "group": "Power Tools",
            "pages": [
              "additional-info/mcps",
              "additional-info/existing-projects",
              "additional-info/deployments"
            ]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Help & Resources",
            "pages": [
              "support/index",
              "support/github-discussions",
              "support/hackathon-help",
              "support/faq"
            ]
          }
        ]
      },
      {
        "tab": "Bank Data Sharing",
        "openapi": [
          "/api-reference/bank-data-sharing/uae-account-information-openapi.yaml",
          "/api-reference/bank-data-sharing/uae-atm-openapi.yaml",
          "/api-reference/bank-data-sharing/uae-product-openapi.yaml"
        ]
      },
      {
        "tab": "Bank Service Initiation",
        "openapi": [
          "/api-reference/bank-service-initiation/uae-account-opening-service-initiation-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-bank-initiation-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-confirmation-of-payee-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-fx-service-initiation-openapi.yaml"
        ]
      },
      {
        "tab": "Insurance",
        "openapi": "/api-reference/insurance/uae-insurance-openapi.yaml"
      }
