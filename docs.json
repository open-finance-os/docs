{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#DC2626",
    "light": "#EF4444",
    "dark": "#991B1B"
  },
  "favicon": "/adcb-favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "Bank Data Sharing",
        "openapi": [
          "/api-reference/bank-data-sharing/uae-account-information-openapi.yaml",
          "/api-reference/bank-data-sharing/uae-atm-openapi.yaml",
          "/api-reference/bank-data-sharing/uae-product-openapi.yaml"
        ]
      },
      {
        "tab": "Bank Service Initiation", 
        "openapi": [
          "/api-reference/bank-service-initiation/uae-account-opening-service-initiation-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-bank-initiation-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-confirmation-of-payee-openapi.yaml",
          "/api-reference/bank-service-initiation/uae-fx-service-initiation-openapi.yaml"
        ]
      },
      {
        "tab": "Insurance",
        "openapi": "/api-reference/insurance/uae-insurance-openapi.yaml"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/adcb-light.png",
    "dark": "/logo/adcb-dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
